{
  "name": "@donleeit/protos",
  "version": "1.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "don",
  "files": [
    "pb",
    "proto"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "gen": "npx protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./pb/typescript --proto_path=./proto ./proto/**/*.proto --ts_proto_opt=esModuleInterop=true",
    "build": "tsc -p tsconfig.json",
    "publish": "npm publish --access public"
  },
  "devDependencies": {
    "glob": "^11.0.3"
  },
  "dependencies": {
    "@donleeit/protos": "1.0.1",
    "@grpc/grpc-js": "^1.13.4",
    "ts-proto": "^2.7.7"
  }
}